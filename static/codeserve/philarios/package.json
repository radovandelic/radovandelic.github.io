{
  "name": "philarios",
  "version": "0.1.0",
  "description": "Where it all comes from is a mystery, It's like the changing of the seasons and the tides of the sea.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src & mocha test"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radovandelic/philarios.git"
  },
  "author": "Radovan Delic <r.delic@net.hr>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "country-data": "0.0.31",
    "express": "^4.17.3",
    "file-system": "^2.2.2",
    "flickrapi": "^0.6.0",
    "google-translator": "^1.0.1",
    "morgan": "^1.8.0",
    "pg": "^7.4.0",
    "request": "^2.83.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.2",
    "eslint": "^3.1.1",
    "mocha": "^5.0.0",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/radovandelic/philarios/issues"
  },
  "homepage": "https://philarios.ml"
}